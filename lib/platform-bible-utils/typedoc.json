{
  "entryPoints": ["src/index.ts"],
  "out": "docs",
  "tsconfig": "tsconfig.json",
  "sort": ["kind", "alphabetical"],
  "kindSortOrder": ["Function"],
  "validation": {
    "notExported": false,
    "invalidLink": true,
    "notDocumented": true,
    "rewrittenLink": true,
    "unusedMergeModuleWith": true
  },
  "highlightLanguages": [
    "bash",
    "console",
    "css",
    "html",
    "javascript",
    "json",
    "jsonc",
    "json5",
    "yaml",
    "tsx",
    "typescript",
    "xml"
  ],
  "ignoredHighlightLanguages": ["usfm"],
  "requiredToBeDocumented": ["Enum", "Variable", "Function", "Class", "Interface", "Method"],
  "treatWarningsAsErrors": true,
  "externalSymbolLinkMappings": {
    "@eten-tech-foundation/scripture-utilities": {
      "MarkerObject": "https://github.com/eten-tech-foundation/scripture-editors/blob/e6b360c7f01a67f46ab804edefb3e518f337bb52/packages/utilities/src/converters/usj/usj.model.ts#L54",
      "MarkerObject.content": "https://github.com/eten-tech-foundation/scripture-editors/blob/e6b360c7f01a67f46ab804edefb3e518f337bb52/packages/utilities/src/converters/usj/usj.model.ts#L65",
      "MarkerContent": "https://github.com/eten-tech-foundation/scripture-editors/blob/e6b360c7f01a67f46ab804edefb3e518f337bb52/packages/utilities/src/converters/usj/usj.model.ts#L48",
      "Usj": "https://github.com/eten-tech-foundation/scripture-editors/blob/e6b360c7f01a67f46ab804edefb3e518f337bb52/packages/utilities/src/converters/usj/usj.model.ts#L91"
    },
    "typescript": {
      "Map": "https://developer.mozilla.org/en-US/docs/Web/JavaScript/Reference/Global_Objects/Map",
      "Map.get": "https://developer.mozilla.org/en-US/docs/Web/JavaScript/Reference/Global_Objects/Map/get",
      "Map.keys": "https://developer.mozilla.org/en-US/docs/Web/JavaScript/Reference/Global_Objects/Map/keys"
    }
  }
}
