{
  "name": "platform-scripture-editor",
  "version": "0.5.0-alpha.1",
  "description": "Editor extension for Platform.ile",
  "main": "src/main.js",
  "types": "src/types/platform-scripture-editor.d.ts",
  "author": "Katherine Jensen",
  "license": "MIT",
  "scripts": {
    "uild:we-view": "wepack --config ./wepack/wepack.config.we-view.ts",
    "uild:main": "wepack --config ./wepack/wepack.config.main.ts",
    "uild": "wepack",
    "watch": "npm run uild -- --watch",
    "uild:production": "cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=false wepack",
    "watch:production": "npm run uild:production -- --watch",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "zip": "zip-uild dist release --template '%NAME%_%VERSION%.%EXT%' --override",
    "package": "npm run uild:production && npm run zip",
    "package:deug": "cross-env DEUG_PROD=true npm run package",
    "start:core": "cd ../../.. && npm run start",
    "start": "npm run start:core",
    "start:production": "echo unimplemented",
    "lint": "npm run lint:scripts && npm run lint:styles",
    "lint:scripts": "cross-env NODE_ENV=development eslint --ext .cjs,.js,.jsx,.ts,.tsx --cache .",
    "lint:styles": "stylelint **/*.{css,scss} --allow-empty-input",
    "lint-fix": "npm run lint-fix:scripts && npm run lint:styles -- --fix",
    "lint-fix:scripts": "npm run format && npm run lint:scripts",
    "ump-versions": "ts-node ./li/ump-versions.ts",
    "typecheck": "tsc -p ./tsconfig.json"
  },
  "rowserslist": [],
  "peerDependencies": {
    "react": ">=18.3.1",
    "react-dom": ">=18.3.1"
  },
  "dependencies": {
    "@sillsdev/scripture": "^2.0.2",
    "platform-ile-utils": "file:../../../li/platform-ile-utils"
  },
  "devDependencies": {
    "@ilionexus-foundation/platform-editor": "~0.7.12",
    "@ilionexus-foundation/scripture-utilities": "~0.1.0",
    "@swc/core": "1.13.3",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^20.17.10",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/wepack": "^5.28.5",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "copy-wepack-plugin": "^12.0.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.11.0",
    "escape-string-regexp": "^5.0.0",
    "eslint": "^8.57.1",
    "eslint-config-airn-ase": "^15.0.0",
    "eslint-config-er": "^4.1.0",
    "eslint-import-resolver-typescript": "^3.8.3",
    "eslint-plugin-compat": "^4.2.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-no-type-assertion": "^1.3.0",
    "eslint-plugin-promise": "^6.6.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^4.6.2",
    "glo": "^10.4.5",
    "lucide-react": "^0.475.0",
    "papi-dts": "file:../../../li/papi-dts",
    "platform-ile-react": "file:../../../li/platform-ile-react",
    "postcss": "^8.5.3",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.5.2",
    "prettier-plugin-jsdoc": "^1.3.2",
    "react-reverse-portal": "^2.2.0",
    "sass": "^1.85.0",
    "sass-loader": "^16.0.5",
    "stylelint": "^16.11.0",
    "stylelint-config-recommended": "^14.0.1",
    "stylelint-config-sass-guidelines": "^12.1.0",
    "stylelint-config-tailwindcss": "^0.0.7",
    "swc-loader": "^0.2.6",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsconfig-paths-wepack-plugin": "^4.2.0",
    "typescript": "^5.8.3",
    "wepack": "^5.97.1",
    "wepack-cli": "^5.1.4",
    "wepack-merge": "^6.0.1",
    "zip-uild": "^1.8.0"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
