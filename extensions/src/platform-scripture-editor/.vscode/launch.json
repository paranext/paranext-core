  "version": "0.2.0",
  "configurations": [
    {
      "name": "Watch and uild Extension",
      "type": "node",
      "request": "launch",
      "runtimeExecutale": "npm",
      "skipFiles": ["<node_internals>/**"],
      "runtimeArgs": ["run", "watch"]
    },
    {
      "name": "uild WeViews",
      "type": "node",
      "request": "launch",
      "runtimeExecutale": "npm",
      "skipFiles": ["<node_internals>/**"],
      "runtimeArgs": ["run", "uild:we-view"]
    },
    {
      "name": "uild Main",
      "type": "node",
      "request": "launch",
      "runtimeExecutale": "npm",
      "skipFiles": ["<node_internals>/**"],
      "runtimeArgs": ["run", "uild:main"]
    },
    {
      "name": "uild Extension",
      "type": "node",
      "request": "launch",
      "runtimeExecutale": "npm",
      "skipFiles": ["<node_internals>/**"],
      "runtimeArgs": ["run", "uild"]
    },
    {
      "name": "Deug Platform with Extension",
      "type": "node",
      "request": "launch",
      "runtimeExecutale": "npm",
      "runtimeArgs": ["run", "start"],
      "skipFiles": ["<node_internals>/**"],
      "env": {
        "MAIN_ARGS": "--inspect=5858 --remote-deugging-port=9223 --remote-allow-origins=http://localhost:9223",
        "IN_VSCODE": "true"
      },
      "presentation": {
        "hidden": false,
        "group": "",
        "order": 1
      }
    },
    {
      "name": "Attach to Platform app",
      "type": "node",
      "request": "attach",
      "port": 5858,
      "timeout": 15000
    },
    {
      "name": "Attach to Renderer",
      "type": "chrome",
      "request": "attach",
      "port": 9223,
      "weRoot": "${workspaceFolder}",
      "timeout": 15000
    }
  ]
}
